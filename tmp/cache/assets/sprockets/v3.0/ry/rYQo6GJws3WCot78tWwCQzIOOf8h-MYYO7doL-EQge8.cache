{:uriI"—file://app/assets/javascripts/angular/templates/users/signup.html?type=text/ng-html&id=6160d5d227b0cdb8f2507780e85c2443a9674eccf0dd413065fe26c49bac46c0:ET:load_pathI"app/assets/javascripts;T:filenameI"?app/assets/javascripts/angular/templates/users/signup.html;T:	nameI"#angular/templates/users/signup;T:logical_pathI"(angular/templates/users/signup.html;T:content_typeI"text/ng-html;T:sourceI"J<h4>SIGN UP</h4>

<form ng-submit="user.signUp()">

  <input
    type="text"
    name="username"
    ng-model="user.username"
    required="required" />

  <div ng-messages="form.username.$touched">
    <div ng-messages="form.username.$error.required">
      Username required!
    </div>
  </div>

  <input
    type="email"
    name="email"
    ng-model="user.email"
    required="required" />

  <div ng-messages="form.username.$touched">
    <div ng-messages="form.username.$error.required">
      Email required!
    </div>
  </div>

  <input
    type="password"
    name="password"
    ng-model="user.password"
    required="required"
    minlength="6" />

  <div ng-messages="form.password.$touched">
    <div ng-messages="form.password.$error.required">
      Password required!
    </div>
    <div ng-messages="form.password.$error.minlength">
      Password must be more than 6 characters!
    </div>
  </div>

  <button type="submit" class="btn btn-primary">Sign Up</button>

</form>

<p>Already have an account? <a href="#" ui-sref="signin" ui-sref-active="active">Sign in here.</a></p>
