{:uriI"—file://app/assets/javascripts/angular/templates/users/signin.html?type=text/ng-html&id=7d8bbb51f20ead5e6cb65f933bdbd8b017b0f545313ed54ef91aecb0b470ab9a:ET:load_pathI"app/assets/javascripts;T:filenameI"?app/assets/javascripts/angular/templates/users/signin.html;T:	nameI"#angular/templates/users/signin;T:logical_pathI"(angular/templates/users/signin.html;T:content_typeI"text/ng-html;T:sourceI"½<h4>SIGN IN</h4>

<form ng-submit="user.signIn()">

  <input
    type="text"
    name="username"
    ng-model="user.username"
    required="required"
    ng-model="ctrl.username"
    placeholder="Username" />

  <div ng-if="form.username.$touched">
    <div ng-messages="form.username.$error.required">
      Username required!
    </div>
  </div>

  <input
    type="password"
    name="password"
    ng-model="user.password"
    required="required"
    minlength="6"
    ng-model="ctrl.password"
    placeholder="Password" />

  <div ng-if="form.password.$touched">
    <div ng-messages="form.password.$error.required">
      Password required!
    </div>
    <div ng-messages="form.password.$error.minlength">
      Password must be more than 6 characters!
    </div>
  </div>

  <button type="submit" class="btn btn-primary">Sign In</button>

</form>

<p>Don't have an account? <a href="#" ui-sref="signup" ui-sref-active="active">Sign up here.</a></p>
