{:uriI"—file://app/assets/javascripts/angular/templates/users/signup.html?type=text/ng-html&id=a782abd03ed5016e3dbf54177a1ab2cf005737abc0779a103af5fc11e66a0923:ET:load_pathI"app/assets/javascripts;T:filenameI"?app/assets/javascripts/angular/templates/users/signup.html;T:	nameI"#angular/templates/users/signup;T:logical_pathI"(angular/templates/users/signup.html;T:content_typeI"text/ng-html;T:sourceI"†<h4>SIGN UP</h4>

<form ng-submit="user.signUp()">

  <input
    type="text"
    name="username"
    ng-model="user.username"
    required="required"
    placeholder="Username" />

  <div ng-if="form.username.$touched">
    <div ng-messages="form.username.$error.required">
      Username required!
    </div>
  </div>

  <input
    type="email"
    name="email"
    ng-model="user.email"
    required="required"
    placeholder="Email" />

  <div ng-if="form.username.$touched">
    <div ng-messages="form.username.$error.required">
      Email required!
    </div>
  </div>

  <input
    type="password"
    name="password"
    ng-model="user.password"
    required="required"
    minlength="6"
    placeholder="Password" />

  <div ng-if="form.password.$touched">
    <div ng-messages="form.password.$error.required">
      Password required!
    </div>
    <div ng-messages="form.password.$error.minlength">
      Password must be more than 6 characters!
    </div>
  </div>

  <button type="submit" class="btn btn-primary">Sign Up</button>

</form>

<p>Already have an account? <a href="#" ui-sref="signin" ui-sref-active="active">Sign in here.</a></p>
